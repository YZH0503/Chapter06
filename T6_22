Function:打印指定大小的星号方形图案
*/
#include <iostream>

using namespace std;

int main()
{
    int side;
    cin>>side;
    for(int i=0;i<side;i++)
    {
        for(int x=0;x<side;x++)
        {
            cout<<"*";
        }
        cout<<"\n";
    }
    return 0;
}
