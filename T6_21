Function:判断一系列整数对中后一个是否为前一个的倍数
*/
#include <iostream>

using namespace std;

int main()
{
    int a,b;
    while(1)
    {
       cin>>a;
    cin>>b;
    if(b%a==0)
    {
        cout<<"true";
    }
    else
    {
        cout<<"false";
    }
    }

    return 0;
}
