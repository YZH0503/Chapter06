Function:打印特定大小的由任意图案组成的方形
*/
#include <iostream>

using namespace std;

int main()
{
    int side;
    cin>>side;
    char f;
    cin>>f;
    for(int i=0;i<side;i++)
    {
        for(int x=0;x<side;x++)
        {
            cout<<f;
        }
        cout<<"\n";
    }
    return 0;
}
