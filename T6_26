#include <iostream>
#include<cmath>

using namespace std;

int main()
{
    int x,y,z,a,b,c;
    cin>>x>>y>>z>>a>>b>>c;
    cout<<abs(x-a)*3600+abs(y-b)*60+abs(z-c);
      return 0;
}
